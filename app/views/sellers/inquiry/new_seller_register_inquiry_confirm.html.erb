<div class="container">
  <div class ="row">

    <div class="col-md-2 col-0">
    </div>

    <div class="col-md-8 col-12">
      <h2 class="my-4">
        内容をご確認ください。
      </h2>

      <%= form_for @inquiry, url: :sellers_new_seller_register_inquiry_thanks do |f| %>
      <table class="table">
        <tr>
          <th>会社名</th>
          <td>
            <%= f.hidden_field :company_name %>
            <%= @inquiry.company_name %>
          </td>
        </tr>
        <tr>
          <th>電話番号</th>
          <td>
            <%= f.hidden_field :company_tell %>
            <%= @inquiry.company_tell %>
          </td>
        </tr>
        <tr>
          <th>ホームページ</th>
          <td>
            <%= f.hidden_field :company_url %>
            <%= @inquiry.company_url %>
          </td>
        </tr>
        <tr>
          <th>メールアドレス</th>
          <td>
            <%= f.hidden_field :email %>
            <%= @inquiry.email %>
          </td>
        </tr>
        <tr>
          <th>備考</th>
          <td>
            <%= f.hidden_field :contents %>
            <%= @inquiry.contents %>
          </td>
        </tr>
      </table>

      <%= f.submit '問い合わせる', class: 'btn btn-primary' %>
      <%= f.submit '修正する', name: 'correct', class: 'btn btn-secondary' %>

      <% end %>
      
    </div>

    <div class="col-md-2 col-0">
    </div>

  </div>
</div>
