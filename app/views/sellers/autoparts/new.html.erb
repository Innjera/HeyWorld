<div class="container">
  <div class="row">

    <nav class="col-2 bg-light sidebar">
      <%= render 'sellers/shared/sidebar' %>
    </nav>

    <main class="col-10 ml-sm-auto">
    <h1 class="my-3"><i class="fas fa-car"></i>&nbsp;部品登録</h1>
    <%= form_for @autopart, url: :sellers_autoparts do |form| %>
      <%= render 'sellers/autoparts/form', form: form %>
      <%= form.submit "登録する", class:"btn btn-primary my-3" %>
    <% end %>
    </main>

  </div>
</div>
